    "@fontsource/quicksand": "^4.5.6",
    "@fontsource/source-code-pro": "^4.5.6",
    "@fontsource/yellowtail": "^4.5.7",
    "@fontsource/yeseva-one": "^4.5.5",
    "framer-motion": "^6.3.3",
    "hero-patterns": "^2.1.0",
    "next": "12.1.5",
    "next-themes": "^0.1.1",
    "react": "18.1.0",
    "react-component-export-image": "^1.0.6",
    "react-dom": "18.1.0",
